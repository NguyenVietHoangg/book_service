        script(src='/admin/plugins/cropper/jquery-cropper_files/main.js.download')
        script.
            $(function  () {
              $('.cropper-container.cropper-bg').hide();
            })
            $(function(){
              $('.download-crop').click(function() {
                $('.btn.btn-secondary').click();
                setTimeout(() => {
                  $.ajax({
                    url: '/admin/media/add',
                    type: 'post',
                    data: {
                      encode64img: $('#download').attr('href'),
                      name: 'cropfile',
                      width: 480,
                      height: 271,
                      is_watermark: $('#is_watermark').is(":checked") ? 'checked' : false,
                      text_watermark: $('#text_watermark').val()
                    },
                    success: function(res) {
                      parent.$(parent.document).trigger('eventhandler');
                    }
                  });
                }, 1500);   
              })
            });
