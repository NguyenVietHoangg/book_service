{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run watch ./node_modules/.bin/babel-node ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production ./node_modules/.bin/babel-node ./dist/bin/www",
    "migratedb": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "resetdb": "node src/database/config/createdb.js && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "@babel/compat-data": "^7.10.1",
